{
  "$schema": "https://github.com/Azure/data-api-builder/releases/download/v0.6.13/dab.draft.schema.json",
  "data-source": {
    "database-type": "mssql",
    "options": {
      "set-session-context": true
    },
    "connection-string": "Data Source=(localdb)\\MSSQLLocalDB;Initial Catalog=SampleDb;Integrated Security=True;Connect Timeout=60;Encrypt=False;TrustServerCertificate=False;ApplicationIntent=ReadWrite;MultiSubnetFailover=False"
  },
  "runtime": {
    "rest": {
      "enabled": true,
      "path": "/api"
    },
    "graphql": {
      "allow-introspection": true,
      "enabled": false,
      "path": "/graphql"
    },
    "host": {
      "mode": "development",
      "cors": {
        "origins": [],
        "allow-credentials": false
      },
      "authentication": {
        "provider": "StaticWebApps"
      }
    }
  },
  "entities": {
    "Customers": {
      "source": {
        "type": "table",
        "object": "[dbo].[Customers]",
        "key-fields": [
          "Id"
        ]
      },
      "permissions": [
        {
          "role": "anonymous",
          "actions": [
            "create",
            "read",
            "update",
            "delete"
          ]
        }
      ],
      "mappings": {
        "City": "city",
        "Id": "id",
        "Location": "location",
        "Name": "name",
        "SpecialRank": "special-rank",
        "State": "state"
      },
      "rest": {
        "path": "/customers"
      }
    },
    "Lines": {
      "source": {
        "type": "table",
        "object": "[dbo].[Lines]",
        "key-fields": [
          "Id"
        ]
      },
      "permissions": [
        {
          "role": "anonymous",
          "actions": [
            "create",
            "read",
            "update",
            "delete"
          ]
        }
      ],
      "mappings": {
        "Id": "id",
        "OrderId": "order-id",
        "PriceEach": "price-each",
        "ProductId": "product-id",
        "Quantity": "quantity"
      },
      "rest": {
        "path": "/lines"
      }
    },
    "Orders": {
      "source": {
        "type": "table",
        "object": "[dbo].[Orders]",
        "key-fields": [
          "Id"
        ]
      },
      "permissions": [
        {
          "role": "anonymous",
          "actions": [
            "create",
            "read",
            "update",
            "delete"
          ]
        }
      ],
      "mappings": {
        "CustomerId": "customer-id",
        "Date": "date",
        "Id": "id"
      },
      "rest": {
        "path": "/orders"
      }
    },
    "Products": {
      "source": {
        "type": "table",
        "object": "[dbo].[Products]",
        "key-fields": [
          "Id"
        ]
      },
      "permissions": [
        {
          "role": "anonymous",
          "actions": [
            "create",
            "read",
            "update",
            "delete"
          ]
        }
      ],
      "mappings": {
        "Id": "id",
        "Name": "name",
        "Price": "price"
      },
      "rest": {
        "path": "/products"
      }
    },
    "Sample": {
      "source": {
        "type": "table",
        "object": "[dbo].[Sample]",
        "key-fields": [
          "Id1",
          "Id2",
          "Id3"
        ]
      },
      "permissions": [
        {
          "role": "anonymous",
          "actions": [
            "create",
            "read",
            "update",
            "delete"
          ]
        }
      ],
      "mappings": {
        "Id1": "id1",
        "Id2": "id2",
        "Id3": "id3",
        "lowercase": "lowercase",
        "Name": "name",
        "Super weird Name-here": "super-weird-name-here",
        "UpperName": "upper-name",
        "Very % strange-like # name @ ": "very-strange-like-name",
        "Weird Name": "weird-name"
      },
      "rest": {
        "path": "/sample"
      }
    },
    "SampleNode": {
      "source": {
        "type": "table",
        "object": "[dbo].[SampleNode]",
        "key-fields": [
          "Id"
        ]
      },
      "permissions": [
        {
          "role": "anonymous",
          "actions": [
            "create",
            "read",
            "update",
            "delete"
          ]
        }
      ],
      "mappings": {
        "Id": "id"
      },
      "rest": {
        "path": "/samplenode"
      }
    },
    "PageCustomers": {
      "source": {
        "type": "stored-procedure",
        "object": "[dbo].[PageCustomers]",
        "parameters": {
          "PageSize": "",
          "StartIndex": ""
        }
      },
      "permissions": [
        {
          "role": "anonymous",
          "actions": [
            "execute"
          ]
        }
      ],
      "mappings": {
        "City": "city",
        "Id": "id",
        "Name": "name",
        "State": "state"
      },
      "rest": {
        "path": "/pagecustomers",
        "methods": [
          "post",
          "get"
        ]
      },
      "graphql": {
        "operation": "mutation"
      }
    },
    "Top10Customers": {
      "source": {
        "type": "stored-procedure",
        "object": "[dbo].[Top10Customers]"
      },
      "permissions": [
        {
          "role": "anonymous",
          "actions": [
            "execute"
          ]
        }
      ],
      "mappings": {
        "City": "city",
        "Id": "id",
        "Location": "location",
        "Name": "name",
        "SpecialRank": "special-rank",
        "State": "state"
      },
      "rest": {
        "path": "/top10customers",
        "methods": [
          "post",
          "get"
        ]
      },
      "graphql": {
        "operation": "mutation"
      }
    }
  }
}